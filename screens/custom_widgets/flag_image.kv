#:kivy 2.2.1

<FlagImage>:

    fit_mode:"scale-down"

    canvas.before:
        Color:
            rgba: (1,1,1,1)
        Rectangle:
            pos:self.pos
            size:self.size
    
    canvas.after:
        Color:
            rgba: self.primary_color
        Line:
            width:2
            rectangle: [self.x,self.y,self.width,self.height]
